{UTF-8}
Wanted Earnings=
	250
	~	USD/Month
	~		|

Exchange fees=
	(Price*Phi parameter)*0.0026+(Taxes/Gamma parameter)-Theta parameter*Exchanges
	~	USD
	~		|

NETETC=
	IF THEN ELSE( Taxes/Gamma parameter > Price*Phi parameter , 0 , 10 )
	~	Exc/Month
	~		|

ETDETC=
	IF THEN ELSE(Taxes/Gamma parameter > Price*Phi parameter, 10 , 0 )
	~	Exc/Month
	~		|

Taxes=
	Price*CC Sold*0/32
	~	USD/Month
	~		|

New investors=
	IF THEN ELSE( Earnings > Wanted Earnings ,CC/Beta paramater*Gamma parameter , CC/Beta paramater\
		*Gamma parameter/64 )
	~	People/Month
	~		|

Theta parameter=
	1
	~	USD/Exc
	~		|

CC Bought=
	DELAY N(IF THEN ELSE( Growth Rate > 0, CC Exchanges/16*Gamma parameter + Investors*Beta paramater\
		*Gamma parameter ,  IF THEN ELSE
	( Exchange fees/Price < 0.0035, 0, CC Exchanges/32*Gamma parameter + Investors*Beta paramater\
		*Gamma parameter)), FINAL TIME, 0 , 3 )
	~	CC/Month
	~		|

Earnings=
	(Price-Price Before)*CC Sold - Exchange fees*Gamma parameter
	~	USD/Month
	~		|

Exchanges= INTEG (
	NETETC-ETDETC,
		0)
	~	Exc
	~		|

CC Sold=
	DELAY N( IF THEN ELSE( Growth Rate < 0 , CC/16*Gamma parameter, CC/32*Gamma parameter\
		), FINAL TIME , 0 , 3 )
	~	CC/Month
	~		|

Phi parameter=
	1
	~	CC
	~		|

Investors= INTEG (
	New investors-Outflow investors,
		0)
	~	People
	~		|

Outflow investors=
	IF THEN ELSE(Earnings < 0 , INTEGER( Investors/16 )*Gamma parameter, INTEGER( Investors\
		/64 )*Gamma parameter )
	~	People/Month
	~		|

Beta paramater=
	5
	~	CC/People
	~		|

Gamma parameter=
	1
	~	1/Month
	~		|

CC= INTEG (
	MAX(CC Bought-CC Sold, 0),
		ICO/2)
	~	CC
	~		|

CC Sold previous=
	DELAY FIXED(CC Sold, 1, 0 )
	~	CC/Month
	~		|

CC Bought previous=
	DELAY FIXED( CC Bought , 1 , 0)
	~	CC/Month
	~		|

CC Exchanges= INTEG (
	MAX(CC Sold-CC Bought, 0),
		ICO/2)
	~	CC
	~		|

ICO=
	4.8e+06
	~	CC
	~		|

Price=
	MAX(Initial Price, Initial Price + Growth rate previous*Price Before + Alpha parameter\
		*(CC Bought previous-CC Sold previous)/CC)
	~	USD/CC
	~		|

Alpha parameter=
	742
	~	USD*Month/(CC)
	~		|

Price Before=
	DELAY FIXED( Price , 1 , Initial Price)
	~	USD/CC
	~		|

Growth rate previous=
	DELAY FIXED(Growth Rate, 1 , 0)
	~	Dmnl
	~		|

Growth Rate=
	IF THEN ELSE( Time > 0 , (Price-Price Before)/Price Before , 1.42 )
	~	Dmnl
	~		|

Initial Price=
	0.5
	~	USD/CC
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 48
	~	Month
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Month
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	Month [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	Month [0,?]
	~	The time step for the simulation.
	|

